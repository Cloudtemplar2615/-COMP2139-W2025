@model COMP2139_ICE.Models.ProjectTask

<h1>Task Details</h1>

<div>
    <dl class="row">
        <dt class="col-sm-2">Title</dt>
        <dd class="col-sm-10">@Model.Title</dd>
        <dt class="col-sm-2">Is Complete</dt>
        <dd class="col-sm-10">@Model.IsComplete</dd>
        <dt class="col-sm-2">Project</dt>
        <dd class="col-sm-10">@Model.Project?.Name</dd>
    </dl>
</div>

<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back to List</a>